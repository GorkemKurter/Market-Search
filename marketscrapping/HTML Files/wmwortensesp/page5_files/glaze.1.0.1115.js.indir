            return content;
        },

        getSubelementContent: function(placeholder, product) {
            var data = placeholder.match(/id="([^"]+)"/),
                content = '';
            if (data) {
                var element = _.find(this.parent.model.get('variant').get('sub_elements'), function(item) {
                    return String(item.glaze_template_sub_element_id) === String(data[1]);
                })

                if (element) {
                    if (element.content && element.content.match('lang_id')) {
                        element.content = JSON.parse(element.content || '[]');

                        let langContent;
                        if (this.appData.getWebsiteLanguage()) {
                            langContent = _.find(element.content, _.bind(function(item) {
                                return item.lang_id === this.appData.getWebsiteLanguage().slug;
                            }, this));
                        }
                        if (!langContent) {
                            langContent = element.content[0];
                        }
                        element.content = langContent.content;
                    }
                    if (element.type === 'text') {
                        content = '<span data-sub-element-id="' + element.id + '">' + element.content + '</span>';
                    }
                    if (element.type === 'button') {
                        content = '<button data-sub-element-id="' + element.id + '">' + element.content + '</button>';
                    }
                    if (element.type === 'image' || element.type === 'html') {
                        content = element.content;
                    }
                }
            }

            if (product) {
                var replacements = [
                    {placeholder: '{PRODUCT.PID}', value: product.pid},
                    {placeholder: '{PRODUCT.SKU}', value: product.sku},
                    {placeholder: '{PRODUCT.PRICE}', value: product.price},
                    {placeholder: '{PRODUCT.CATEGORY_ID}', value: product.category_guid},
                    {placeholder: '{PRODUCT.TITLE}', value: product.name},
                    {placeholder: '{PRODUCT.NAME}', value: product.name},
                    {placeholder: '{PRODUCT.VENDOR}', value: product.vendor},

                    {placeholder: '{PRODUCT.AVAILABILITY}', value: product.availability},
                    {placeholder: '{PRODUCT.STOCK}', value: product.stock_quantity},
                    {placeholder: '{PRODUCT.URL}', value: product.url},
                ];
                _.each(product.custom_data, function(item, key) {
                    replacements.push({
                        placeholder: '{PRODUCT.CUSTOM_FIELD[' + key.toUpperCase() + ']}',
                        value: item
                    })
                });
                _.each(replacements, function(item){
                    content = content.replace(item.placeholder, item.value);
                });
            }

            return content;
        },

        setStyles: function() {
            if (this.model.get('style')) {
                var style = JSON.parse(this.model.get('style'));
                this.$el.css(_.omit(style, 'fontFamily'));

                if (style.fontFamily) {
                    this.$el.css({fontFamily: style.fontFamily.family});

                    if (style.fontFamily.customId) {
                        FontLoader.loadCustomFont(style.fontFamily.customId);
                    } else {
                        FontLoader.loadFonts([style.fontFamily.family]);
                    }
                }
            }
            if (this.model.get('z_index')) this.$el.css({zIndex: this.model.get('z_index')});

            // Animation in
            if (this.model.get('settings') && this.model.get('settings').inEffect) {
                if (this.model.get('settings').inEffect === 'fade') {
                    this.$el.hide();
                    setTimeout(_.bind(function(){
                        this.$el.fadeIn(Number(this.model.get('settings').inDuration));
                    }, this), Number(this.model.get('settings').inDelay))
                }
            }

            // X element
            if (this.model.get('type') === 'x') {
                this.$el.css({cursor: 'pointer'});
            }

            if (this.model.get('type') === 'clipart') {
                this.$el.find('i').attr('aria-hidden', 'true');
            }
        },

        initControls: function() {
            var style = JSON.parse(this.model.get('style'));

            if (style.width) {
                if (this.model.get('type') == 'image') {
                    this.$('img').css({width: '100%'});
                } else if (this.model.get('type') == 'embed') {
                    this.$('iframe').attr('width', style.width);
                    this.$('iframe').attr('height', style.height);
                } else if (this.model.get('type') == 'video') {
                    this.$('iframe').attr('width', style.width);
                    this.$('iframe').attr('height', style.height);
                }
            }

            if (this.model.get('settings') && this.model.get('settings').outEffect) {
                if (this.model.get('settings').outEffect === 'fade' && this.model.get('settings').outDelay) {
                    setTimeout(_.bind(function(){
                        this.$el.fadeOut(Number(this.model.get('settings').outDuration));
                    }, this), Number(this.model.get('settings').outDelay))
                }
            }

            if (this.model.get('type') === 'video') {
                this.videoPlayer = new VideoPlayer(this.model.get('settings'));
                this.videoPlayer.start(this);
            }
        },

        start: function() {
            this.$el.html(this.prepareContent(this.$el.html()));

            var settings = this.model.get('settings') || {};
            if (this.model.get('type') === 'video') {
                if (!settings.sound) {
                    this.videoPlayer.command('mute');
                } else {
                    this.videoPlayer.command('unmute');
                }

                if (settings.autoplay) {
                    this.videoPlayer.command('play');
                }
            }
        },

        updatePosition: function() {
            var $spotlightedElement = this.parent.model.get('variant').getSpotlightedElement(0),
                settings = this.model.get('settings') || {},
                style = JSON.parse(this.model.get('style')),
                scrollingContainer = this.appData.get('website').scroll_container || 'body',
                position = JSON.parse(this.model.get('position'));

            if (this.$el.hasClass('glaze-nestle-video')) {
                return;
            }

            // No spotlight
            if (this.parent.model.get('glaze_template_id') && !this.parent.model.get('page_template_selector_id')) {
                style.position = 'fixed';

                style.left = 0;
                style.top = 0;
                var transforms = [];
                if (this.parent.model.get('position_horizontal') === 'middle') {
                    style.left = '50vw';
                    transforms.push('translateX(-50%)');
                } else if (this.parent.model.get('position_horizontal') === 'right') {
                    style.right = 0;
                    style.left = 'auto'
                }

                if (this.parent.model.get('position_vertical') === 'middle') {
                    style.top = '50vh';
                    transforms.push('translateY(-50%)');
                } else if (this.parent.model.get('position_vertical') === 'bottom') {
                    style.bottom = 0;
                    style.top = 'auto'
                }

                style.transform = transforms.join(' ');
                this.model.set('style', JSON.stringify(style));
                this.$el.css(style);
            } else {
                if (this.model.get('is_centered') && this.model.get('is_centered') != 0) {
                    if (style.position === 'fixed') {
                        style.left = 0;
                        style.top = 'auto';
                        style.marginLeft = 0;
                        style.marginTop = (MiscUtils.getWindowHeight() / 2) - (this.$el.height() / 2);
                    } else {
                        style.left = 0;
                        style.marginLeft = 0;
                        style.marginTop = 0;
                    }
                    style = _.pick(style, 'left', 'top', 'marginLeft', 'marginTop');
                    this.$el.css(style);
                } else if ($spotlightedElement || this.parent.rolledElement) {
                    var $element = this.rolledElement ? $(this.rolledElement) : $spotlightedElement,
                        rect = this.parent.calculateSpotlightArea($element),
                        //rect = $element[0].getBoundingClientRect(),
                        elementPosition = {
                            top: rect.top,
                            right: rect.right,
                            bottom: rect.bottom,
                            left: rect.left,
                            width: rect.width,
                            height: rect.height,
                            x: rect.x,
                            y: rect.y
                        },
                        fixedPosition = false;

                    $element.parents().each(function(){
                        if ($(this).css('position') === 'fixed') {
                            fixedPosition = true;
                        }
                    });

                    if (fixedPosition || style.position === 'fixed') {
                        elementPosition.top += $(scrollingContainer).scrollTop();
                    }

                    if (!$element.length) {
                        // setTimeout(_.bind(this.updatePosition, this), 100);
                        return;
                    }

                    if (settings.shapeOrigin === 'tr') {
                        elementPosition.left += $element.outerWidth();
                    }
                    if (settings.shapeOrigin === 'bl') elementPosition.top += $element.outerHeight();
                    if (settings.shapeOrigin === 'br') {
                        elementPosition.top += $element.outerHeight();
                        elementPosition.left += $element.outerWidth();
                    }

                    if (settings.positionOrigin === 'tr') elementPosition.left -= this.$el.outerWidth();
                    if (settings.positionOrigin === 'bl') elementPosition.top -= this.$el.outerHeight();
                    if (settings.positionOrigin === 'br') {
                        elementPosition.top -= this.$el.outerHeight();
                        elementPosition.left -= this.$el.outerWidth();
                    }

                    if (style.position === 'fixed') {
                        style.marginLeft = elementPosition.left + (position && position.left ? position.left : 0);
                        style.marginTop = elementPosition.top + (position && position.top ? position.top : 0);
                        style.left = 'auto';
                        style.top = 'auto';
                        style = _.pick(style, 'left', 'top', 'marginLeft', 'marginTop');
                    } else {
                        style.left = elementPosition.left + (position && position.left ? position.left : 0);
                        style.top = elementPosition.top + (position && position.top ? position.top : 0);
                        style = _.pick(style, 'left', 'top');
                    }

                    if (parseInt(style.left) < 0) style.left = 0;
                    if (parseInt(style.left) > 0) {
                        if (this.$el.outerWidth() + parseInt(style.left) > $(window).width()) {
                            style.left = $(window).width() - this.$el.outerWidth();
                        }
                    }

                    this.$el.css(style);
                }
            }

        },

        clickHandle: function(event) {
            var settings = this.model.get('settings') || {},
                converted = false;

            event && event.stopPropagation();

            if (this.model.get('type') === 'x') {
                converted = TrackingManager.checkEngagementConversion(this.parent.model.get('id'), 'inserted_element', this.model);
                Logger.log('Glaze: X element clicked, closing glaze');
                this.parent.hideGlaze(null, converted);
                return;
            }

            if ($(event.target).hasClass('js-close') || $(event.target).parents('.js-close').length) {
                this.$el.removeClass('glaze-nestle-video');
                // this.$el.draggable("destroy");

                this.$('iframe').attr('width', this.videoWidth);
                this.$('iframe').attr('height', this.videoHeight);
                var style = JSON.parse(this.model.get('style'));
                this.$el.css(style);
                this.videoPlayer.command('pause');
                return;
            }

            if (this.model.get('is_tracked') != 0) {
                converted = TrackingManager.checkEngagementConversion(this.parent.model.get('id'), 'inserted_element', this.model);
            } else {
                Logger.log('Glaze: element clicked, but tracking is disabled');
            }

            var subElementId = $(event.target).data('sub-element-id');
            if (subElementId) {
                var subElement = _.find(this.parent.model.get('variant').get('sub_elements'), function(item) {
                    return String(item.id) === String(subElementId);
                });

                if (subElement.action) {
                    var action = subElement.action;
                    settings.actionType = action.type;
                    settings.actionUrl = action.url;
                    settings.actionUrlBlank = action.url_blank;
                    settings.actionGlaze = action.glaze_id;
                    settings.actionGlazeTransition = action.smooth_transition && 'smooth';
                    settings.actionCampaign = action.campaign_id;
                    settings.actionJs = action.code;
                    settings.has_products = action.has_products;
                    settings.product_group_settings = action.product_group_settings;
                    settings.after_products = action.after_products;
                    settings.redirect_url = action.redirect_url;
                    settings.subelement_products = subElement.products;
                }
            }

            var cb = this.parent.parent.openGlazeById,
                context = this.parent.parent;

            if (settings.actionType) {
                if (settings.actionType === 'url') {
                    if (settings.actionUrlBlank) {
                        var win = window.open(settings.actionUrl, '_blank');
                        win.focus();
                    } else {
                        window.location.href = settings.actionUrl;
                    }
                } else if (settings.actionType === 'glaze') {
                    if (settings.actionGlazeTransition && settings.actionGlazeTransition === 'smooth') {
                        this.parent.smoothTransitionGlaze(settings.actionGlaze);
                    } else {
                        Logger.log('Glaze: element clicked with action to open another glaze, closing');
                        this.parent.hideGlaze(function(){
                            cb.call(context, settings.actionGlaze, true);
                        }, converted);
                    }
                } else if (settings.actionType === 'close') {
                    Logger.log('Glaze: element clicked with action to close the glaze, closing');
                    this.parent.hideGlaze(null, converted);
                } else if (settings.actionType === 'close_campaign') {
                    Logger.log('Glaze: element clicked with action to to close campaign, closing');
                    this.parent.hideGlaze(null, converted, {forceClose: true});
                } else if (settings.actionType === 'js') {
                    try {
                        event && event.preventDefault();

                        if (!settings.has_products) {
                            if (settings.actionJs.match('{PRODUCT')) {
                                var products = settings.subelement_products || this.model.get('products') || [];
                                _.each(products, _.bind(function(product) {
                                    eval(this.prepareProductJsCode(settings.actionJs, product));
                                }, this));

                                setTimeout(_.bind(function(){
                                    Logger.log('Glaze: element clicked with JS action without products, closing');
                                    this.parent.hideGlaze(null, converted);
                                }, this), 1000);
                            } else {
                                eval(settings.actionJs);
                            }
                        } else {
                            if (settings.product_group_settings && settings.product_group_settings.length) {
                                var promises = [];
                                _.each(settings.product_group_settings, _.bind(function(productGroup) {
                                    var productsList = settings.subelement_products || this.model.get('products');
                                    var products = _.filter(productsList || [], function(item) {
                                        return String(item.product_group_id) === String(productGroup.id);
                                    });
                                    productGroup.actionCodes = [];
                                    _.each(products, _.bind(function(item) {
                                        productGroup.actionCodes.push(this.prepareProductJsCode(productGroup.code, item));
                                    }, this));
                                    promises.push(productGroup.actionCodes);
                                }, this))

                                if (settings.after_products === 'reload') {
                                    promises.push(['window.location.reload()']);
                                }
                                if (settings.after_products === 'redirect') {
                                    promises.push(['window.location.href = "' + settings.redirect_url + '"']);
                                }
                                this.executePromises(promises);
                            }

                            setTimeout(_.bind(function(){
                                Logger.log('Glaze: element clicked with JS action with products, closing');
                                this.parent.hideGlaze(null, converted);
                            }, this), 1000);
                        }
                    } catch(err) {
                        console.warn('Error in Custom Javascript Action on click element');
                        console.warn(err);
                    }
                }
            }

            if ($(event.target).attr('data-close-glaze') || $(event.target).parents('[data-close-glaze]').length) {
                Logger.log('Glaze: data-close-glaze element clicked, closing');
                this.parent.hideGlaze(null, false);
            }

            var glazeToTransition,
                transitionType = $(event.target).attr('data-transition-type') || 'smooth';

            if ($(event.target).attr('data-transition') === 'next' || $(event.target).parents('[data-transition="next"]').length) {
                glazeToTransition = this.parent.appData.getNextGlaze(this.parent.model.get('id'));
                if (glazeToTransition) {
                    if (transitionType === 'smooth') {
                        this.parent.smoothTransitionGlaze(glazeToTransition.id);
                    } else {
                        Logger.log('Glaze: data-transition element clicked, closing');
                        this.parent.hideGlaze(function(){
                            cb.call(context, glazeToTransition.id, true);
                        }, converted);
                    }
                }
            }
            if ($(event.target).attr('data-transition') === 'prev' || $(event.target).parents('[data-transition="prev"]').length) {
                glazeToTransition = this.parent.appData.getPrevGlaze(this.parent.model.get('id'));
                if (transitionType === 'smooth') {
                    this.parent.smoothTransitionGlaze(glazeToTransition.id);
                } else {
                    Logger.log('Glaze: data-transition element clicked, closing');
                    this.parent.hideGlaze(function(){
                        cb.call(context, glazeToTransition.id, true);
                    }, converted);
                }
            }
        },

        executePromises: function(promisesCodeGroups) {
            if (!promisesCodeGroups.length) {
                return;
            }
            var promisesCodeGroup = promisesCodeGroups.shift(),
                promises = [];
            _.each(promisesCodeGroup, function(item) {
                promises.push(eval(item));
            });
            Promise.all(promises).then(_.bind(function() {
                this.executePromises(promisesCodeGroups);
            }, this));
        },

        prepareProductJsCode: function(actionCode, product) {
            var replacements = [
                {placeholder: '{PRODUCT.PID}', value: (product.pid || '').trim()},
                {placeholder: '{PRODUCT.SKU}', value: (product.sku || '').trim()},
                {placeholder: '{PRODUCT.CATEGORY_ID}', value: (product.category_guid || '').trim()},
                {placeholder: '{PRODUCT.NAME}', value: (product.name || '').trim()},
                {placeholder: '{PRODUCT.VENDOR}', value: (product.vendor || '').trim()},
                {placeholder: '{PRODUCT.CHANNEL_ID}', value: (product.custom_data && product.custom_data.channel_id || '').trim()},
                {placeholder: '{PRODUCT.OFFER_ID}', value: (product.custom_data && product.custom_data.offer_id || '').trim()},
                {placeholder: '{PRODUCT.SOURCE_ID}', value: (product.custom_data && product.custom_data.source_id || '').trim()},
                {placeholder: '{PRODUCT.PVID}', value: (product.custom_data && product.custom_data.pvid || '').trim()},
            ], replacedActionJs = actionCode;

            _.each(replacements, function(item) {
                replacedActionJs = replacedActionJs.replace(item.placeholder, item.value);
            });

            if (replacedActionJs.match(/JS:(.*):ENDJS/)) {
                replacedActionJs = replacedActionJs.replace(/JS:.*:ENDJS/, eval(replacedActionJs.match(/JS:(.*):ENDJS/)[1]));
            }

            return replacedActionJs;
        },

        rollover: function(event) {
            var settings = this.model.get('settings') || {};
            if (!settings.rolloverEnabled) return;
            settings.rolloverStyle && this.$el.css(settings.rolloverStyle);
        },

        rollout: function(event) {
            var style = JSON.parse(this.model.get('style'));
            style.left = this.$el.css('left');
            style.top = this.$el.css('top');

            if (this.model.get('is_centered') && this.model.get('is_centered') != 0) {
                style = _.omit(style, 'top', 'left', 'marginTop', 'marginLeft');
            }

            style = _.omit(style, 'fontFamily');
            this.$el.css(style);
        },

        onGlazeHide: function() {
            if (this.model.get('type') == 'video') {
                var $iframe = this.$('iframe');
                if (!$iframe.length) return;

                if (this.model.get('settings').nestle && this.videoPlayer.get('playing')) {
                    this.videoWidth = this.$('iframe').width();
                    this.videoHeight = this.$('iframe').height();

                    this.$el.css({
                        top: 0,
                        left: 0
                    });

                    this.$el.addClass('glaze-nestle-video');
                    // this.$el.draggable();
                    this.$('iframe').attr('width', '100%');
                    this.$('iframe').attr('height', '100%');
                    return;
                }

                switch (this.model.get('settings').type) {
                    case 'youtube': PostMessageManager.youTubeCommand($iframe[0], 'pauseVideo'); break;
                    case 'vimeo': PostMessageManager.vimeoCommand($iframe[0], 'pause'); break;
                    case 'dm': PostMessageManager.dailyMotionCommand($iframe[0], 'pause'); break;
                }
            }
            console.log('ELEMENT HIDE');
            this.$el.hide();
        },

        onGlazeShow: function() {
            this.$el.show();

            if (this.model.get('type') == 'video') {
                var $iframe = this.$('iframe');
                if (!$iframe.length) return;
                if (this.model.get('settings').nestle) {
                    this.$el.removeClass('glaze-nestle-video');
                    // this.$el.draggable("destroy");

                    this.$('iframe').attr('width', this.videoWidth);
                    this.$('iframe').attr('height', this.videoHeight);
                    var style = JSON.parse(this.model.get('style'));
                    this.$el.css(style);
                    return;
                }

                switch (this.model.get('settings').type) {
                    case 'youtube': PostMessageManager.youTubeCommand($iframe[0], 'playVideo'); break;
                    case 'vimeo': PostMessageManager.vimeoCommand($iframe[0], 'play'); break;
                    case 'dm': PostMessageManager.dailyMotionCommand($iframe[0], 'play'); break;
                }
            }
        },

        isVisible: function() {
            var style = JSON.parse(this.model.get('style')),
                fixedPosition = style.position === 'fixed';

            if (fixedPosition) {
                var rect = this.$el[0].getBoundingClientRect();
                var viewHeight = Math.max(document.documentElement.clientHeight, window.innerHeight);
                return !(rect.bottom < 0 || rect.top - viewHeight >= 0) && !(rect.left < 0);
            }

            var topOfElement = this.$el.offset().top,
                bottomOfElement = this.$el.offset().top + this.$el.outerHeight(),
                bottomOfScreen = $(window).scrollTop() + $(window).innerHeight(),
                topOfScreen = $(window).scrollTop();

            return (bottomOfScreen > topOfElement) && (topOfScreen < bottomOfElement);
        },

        prepareContent: function(content) {
            var placeholders = content.match(/\{[^\}]+\}/gi);
            if (!placeholders || !placeholders.length ||
                !this.appData.get('website').page_templates ||
                !this.appData.get('website').page_templates.length) {
                return content;
            }
            var selectors = _.reduce(this.appData.get('website').page_templates, function(selectors, item){
                if (!item.selectors || !item.selectors.length) {
                    return selectors;
                }
                _.each(item.selectors, function(selector){
                    selectors.push(selector);
                })
                return selectors;
            }, [])

            _.each(placeholders, _.bind(function(placeholder){
                var selector = _.find(selectors, function(item){
                    return '{' + item.name + '}' === placeholder
                });
                if (selector) {
                    var text = '';
                    if (selector.type === 'JavaScript') {
                        selector.code = selector.code.replace('~HIGHLIGHTED_ELEMENT~', this.parent.model.get('variant').getSelector(0));
                        try {
                            text = eval(selector.code);
                        } catch(e) {
                            console.warn('Error in Selector');
                            console.warn(selector.code);
                            console.warn(err);
                        }
                        content = content.replace(placeholder, text);
                    }
                    if (selector.type === 'CSS') {
                        var $el = $(selector.code);
                        if (!$el.length) {
                            Logger.log('Glaze: placeholder selector element not found: ' + selector.code);
                        } else {
                            text = $el.html();
                        }
                        content = content.replace(placeholder, text);
                    }

                }
            }, this))

            return content;
        },

        remove: function() {
