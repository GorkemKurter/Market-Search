            if (this.initialData.get('website') && this.initialData.get('website').load_condition) {
                Logger.log('Glaze: website load condition enabled, waiting to be fulfilled');
                let pass = false;
                try {
                    pass = eval(this.initialData.get('website').load_condition);
                } catch(e) {
                    console.warn('Glaze: website load condition code error');
                    console.log(e);
                    console.log(this.initialData.get('website').load_condition);
                }

                if (!pass) {
                    setTimeout(_.bind(function() {
                        this.checkWebsiteLoadCondition(callback);
                    }, this), 500);
                    return;
                } else {
                    Logger.log('Glaze: website load condition done');
                    callback();
                }
            } else {
                callback();
            }
        }

