            mainUrl.deleteQueryParam('last_account');
            mainUrl.deleteQueryParam('last_url');

            var data = {
                origin: mainUrl.protocol() + '://' + mainUrl.host(),
                slug: mainUrl.path() + mainUrl.query(),
                query: mainUrl.query(),
                accountId: Settings.accountId,
                hash: window.location.hash
            };

            if(data.slug === this.slug && data.hash === this.hash) return;

            Logger.log('Glaze: URL has changed, restart');

            this.checkWebsiteLoadCondition(_.bind(function() {
                this.slug = data.slug;
                this.hash = data.hash;
                ConditionChecker.clearDeferredTimeouts();
                TrackingManager.trackPage();
                TrackingManager.checkPageConversion();
                TrackingManager.checkWaitingSales();

                var previewGlaze = MiscUtils.getParameterByName('preview_glaze') || MiscUtils.sessionStorage().getItem('preview_glaze');
                if(previewGlaze) {
                    MiscUtils.sessionStorage().setItem('preview_glaze', previewGlaze);
                    data.preview = 1;
                }

                data._glz_sid = MiscUtils.localStorage().getItem('_glz_sid') || '';
                data._glz_ssid = MiscUtils.sessionStorage().getItem('_glz_ssid') || '';

                $.ajax({
                    type: 'get',
                    url: Settings.apiHost + Settings.api.clientData,
                    data: data,
                    success: _.bind(function(data) {
                        this.initialData.set(data.result);
                        this.mainView.afterRender();
