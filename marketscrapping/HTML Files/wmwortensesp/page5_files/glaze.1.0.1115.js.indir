define('utils/PostMessageManager',['require','jquery','underscore','utils/Logger','backbone'],function(require){

    var $ = require('jquery'),
        _ = require('underscore'),
        Logger = require('utils/Logger'),
        Backbone = require('backbone');

    var PostMessageManager = Backbone.Model.extend({
        initialize: function() {
            window.addEventListener('message', _.bind(this.receiveMessage, this));
        },

        receiveMessage: function(event) {
            //Logger.log(event);
        },

        youTubeCommand: function(iframe, func, args) {
            iframe.contentWindow.postMessage(JSON.stringify({
                'event': 'command',
                'func': func,
                'args': args || []
            }), '*');
        },

        vimeoCommand: function(iframe, func, args) {
            iframe.contentWindow.postMessage(JSON.stringify({
                "method": func,
                "value": args || []
            }), '*');
        },

        dailyMotionCommand: function(iframe, func, args) {
            iframe.contentWindow.postMessage(JSON.stringify({
                "command": func,
                "parameters": args || []
            }), '*');
        }
    });

    return new PostMessageManager();
});
define('utils/VideoPlayer',['require','jquery','underscore','backbone','utils/Logger','utils/PostMessageManager'],function(require){

    var $ = require('jquery'),
        _ = require('underscore'),
        Backbone = require('backbone'),
        Logger = require('utils/Logger'),
        PostMessageManager = require('utils/PostMessageManager');

    return Backbone.Model.extend({
        parentView: null,
        queue: [],
        youtubePlayer: null,

        defaults: {
            type: 'youtube',
            playing: null,
            paused: null,
            ready: false
        },

        initialize: function() {
            Backbone.Model.prototype.initialize.apply(this, arguments);
            this.listenTo(this, 'change:ready', _.bind(this.onReady, this));
        },

        start: function(view) {
            this.parentView = view;

            switch(this.get('type')) {
                case 'youtube':
                    this.initYouTubePlayer();
                    break;
                case 'vimeo':
                    window.addEventListener('message', _.bind(this.initVimeoPlayer, this), false);
                    break;
                case 'dm':
                    window.addEventListener('message', _.bind(this.initDailyMotionPlayer, this), false);
                    break;
            }
        },

        command: function(command) {
            if(!_.isFunction(this[command])) {
                Logger.log('Glaze: video player command not found (' + command + ')');
                return;
            }

            if(this.get('ready')) {
                this[command]();
            } else {
                this.queue.push(command);
            }
        },

        pause: function() {
            Logger.log('Glaze: video player pause');
            switch(this.get('type')) {
                case 'youtube':
                    this.youtubePlayer.pauseVideo();
                    break;
                case 'vimeo':
                    PostMessageManager.vimeoCommand(this.parentView.$('iframe')[0], 'pause');
                    break;
                case 'dm':
                    PostMessageManager.dailyMotionCommand(this.parentView.$('iframe')[0], 'pause');
                    break;
            }
        },

        play: function() {
            Logger.log('Glaze: video player play');
            switch(this.get('type')) {
                case 'youtube':
                    this.youtubePlayer.playVideo();
                    break;
                case 'vimeo':
                    PostMessageManager.vimeoCommand(this.parentView.$('iframe')[0], 'play');
                    break;
                case 'dm':
                    PostMessageManager.dailyMotionCommand(this.parentView.$('iframe')[0], 'play');
                    break;
            }
        },

        mute: function() {
            Logger.log('Glaze: video player mute');
            switch(this.get('type')) {
                case 'youtube':
                    this.youtubePlayer.mute();
                    break;
                case 'vimeo':
                    PostMessageManager.vimeoCommand(this.parentView.$('iframe')[0], 'setVolume', [0]);
                    break;
                case 'dm':
                    PostMessageManager.dailyMotionCommand(this.parentView.$('iframe')[0], 'mute');
                    break;
            }
        },

        unmute: function() {
            Logger.log('Glaze: video player unmute');
            switch(this.get('type')) {
                case 'youtube':
                    this.youtubePlayer.unMute();
                    this.youtubePlayer.setVolume(100);
                    break;
                case 'vimeo':
                    PostMessageManager.vimeoCommand(this.parentView.$('iframe')[0], 'setVolume', [1]);
                    break;
                case 'dm':
                    PostMessageManager.dailyMotionCommand(this.parentView.$('iframe')[0], 'unmute');
                    break;
            }
        },

        initYouTubePlayer: function() {
            if(!window.onYouTubePlayerAPIReady) {
                window.onYouTubePlayerAPIReady =  _.bind(this.youtubeApiReady, this);
            } else {
                setTimeout(_.bind(this.youtubeApiReady, this), 300);
                return;
            }

            // Load YouTube Frame API
            (function() { // Closure, to not leak to the scope
                var s = document.createElement("script");
                s.src = (window.location.protocol === 'https:' ? 'https' : 'http') + "://www.youtube.com/player_api";
                var before = document.getElementsByTagName("script")[0];
                before.parentNode.insertBefore(s, before);
            })();
        },

        youtubeApiReady: function() {
            var $iframe = this.parentView.$('iframe');
            $iframe.attr('id', this.parentView.cid);

            this.youtubePlayer = new YT.Player(this.parentView.cid, {
                events: {
                    onReady: _.bind(function(){
                        this.set('ready', true);
                    }, this),
                    onStateChange: _.bind(this.onYouTubeStateChange, this)
                }
            });
            // window.onYouTubePlayerAPIReady = null;
        },

        initVimeoPlayer: function(event) {
            // Handle messages from the vimeo player only
            if (!(/^https?:\/\/player.vimeo.com/).test(event.origin)) {
                return false;
            }

            var data = JSON.parse(event.data);

            if(data.event == 'ready') {
                this.set('ready', true);
                PostMessageManager.vimeoCommand(this.parentView.$('iframe')[0], 'addEventListener', 'pause');
                PostMessageManager.vimeoCommand(this.parentView.$('iframe')[0], 'addEventListener', 'finish');
                PostMessageManager.vimeoCommand(this.parentView.$('iframe')[0], 'addEventListener', 'playProgress');
            }

            if(data.event == 'playProgress') {
                if(this.get('paused')) {
                    this.unmute();
                }
                this.set({
                    playing: true,
                    paused: false
                });
            }

            if(data.event == 'pause') {
                this.set({
                    playing: false,
                    paused: true
                });
            }
        },

        initDailyMotionPlayer: function(event) {
            // Handle messages from the dailymotion player only
            if (!(/^https?:\/\/www.dailymotion.com/).test(event.origin)) {
                return false;
            }

            if(event.data && event.data == 'event=apiready') {
                this.set('ready', true);
            }
            if(event.data && event.data == 'event=playing') {
                if(this.get('paused')) {
                    this.unmute();
                }
                this.set({
                    playing: true,
                    paused: false
                });
            }
            if(event.data && event.data == 'event=pause') {
                if(this.get('paused')) {
                    this.unmute();
                }
                this.set({
                    playing: false,
                    paused: true
                });
            }
        },

        onYouTubeStateChange: function(event) {
            if(event.data === YT.PlayerState.PLAYING) {
                if(this.get('paused')) {
                    this.unmute();
                }
                this.set({
                    playing: true,
                    paused: false
                });
            } else if(event.data === YT.PlayerState.PAUSED) {
                this.set({
                    playing: false,
                    paused: true
                });
            }

        },

        onReady: function() {
            Logger.log('Glaze: Video ready (' + this.get('type') + ')');

            _.each(this.queue, _.bind(function(command){
                this[command]();
            }, this));
        }
    });

});

