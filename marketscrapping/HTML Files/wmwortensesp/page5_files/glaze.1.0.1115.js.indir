            window.addEventListener('scroll', _.bind(this.updatePositionOnScroll, this), true);
            // $('*').on('scroll.glaze', _.bind(this.scrollOrResize, this));
            // $(window).on('scroll.glaze', _.bind(this.scrollOrResize, this));
            $(window).on('webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend', _.bind(this.onTransition, this));
            $(document).on('mouseleave', _.bind(this.exitIntent, this));

            if (!md.tablet() && !md.phone()) {
                window.addEventListener('click', _.bind(this.clickShape, this), true);
            } else {
                // window.addEventListener('tap', _.bind(this.clickShape, this), true);
                window.addEventListener('click', _.bind(this.clickShape, this), true);
                var ua = window.navigator.userAgent;
                var iOS = ua.match(/iP(ad|hone)/i);
                var webkit = ua.match(/WebKit/i);
                if (iOS && webkit && !ua.match(/CriOS/i)) {
                    document.body.style.cursor = 'pointer';
                }
            }

            this.listenTo(this, 'releaseAsyncCondition', _.bind(function() {
                if (!this.glazeViews.length) {
                    this.afterRender();
                }
            }, this));
            this.setClearCommand();
            this.initPreviewPreloader();
        },

        render: function() {
            this.$el.html(_.template(template)({data: this}));
            this.afterRender();
            return this;
        },

        afterRender: function() {
            if (this.appData.get('website') && this.appData.get('website').excluded) {
                Logger.log('Glaze: you are excluded from this website Glaze visitors. Clear cookies to get back.');
                return;
            }

            var glazes = this.selectGlaze();
            if (_.some(glazes, function(item) {
                return item.isAsync;
            })) {
                return;
            }
            
            let glazeVisible = false;
            if (this.glazeViews && this.glazeViews.length) {
                glazeVisible = _.some(this.glazeViews, function(view) {
                    return view.isShown;
                });
            }
            
            if (!glazeVisible) {
                if (glazes && glazes.length) {
                    this.$el.css({display: 'block'});
                    this.renderGlaze(glazes);
                } else {
                    this.$el.css({display: 'none'});
                }
            }
        },

        renderGlaze: function(glazes, ignoreConditions, doNotRemoveViews) {
            ignoreConditions = ignoreConditions || false;
            doNotRemoveViews = doNotRemoveViews || false;
            if (this.glazeViews.length && !doNotRemoveViews) {
                _.each(this.glazeViews, function(view){
                    view && view.remove();
                });
            }

            glazes = _.filter(glazes, function(glaze) {
                return !!glaze;
            });
            var allRollOnGlazes = _.all(glazes, _.bind(function(glaze){
                var selectedVariant = this.preSelectGlazeVariant(glaze),
                    settings = selectedVariant && JSON.parse(selectedVariant.settings);

                return settings && settings.roll_on;
            }, this));

            var allDeferredGlazes = _.all(glazes, function(glaze) {
                return glaze.isDeferred;
            });

            _.each(glazes, _.bind(function(glaze, index){
                if (allRollOnGlazes || index === 0 ||
                    allDeferredGlazes ||
                    (this.appData.get('website').account.can_have_multiple_campaigns && !this.appData.get('previewGlaze'))) {
                    if (this.abHasGlaze === null) {
                        this.abHasGlaze = TrackingManager.getAB(glaze.campaign_id) || -1;
                        if (this.abHasGlaze != -1) {
                            Logger.log('Glaze: A/B testing, saved A/B version - ' + (this.abHasGlaze == 1 ? 'with glaze' : 'without glaze'));
                        }
                    }

                    if (glaze.forceRestart && !ignoreConditions) {
                        Logger.log('Glaze: restart from', glaze);
                        var model = new Backbone.Model(glaze),
                            selectedVariant = this.preSelectGlazeVariant(glaze);

                        var variantJson = JSON.parse(selectedVariant.json);
                        variantJson.settings = selectedVariant.settings ? JSON.parse(selectedVariant.settings) : {};

                        var variant = new Backbone.Model(variantJson);
                        model.set('variant', variant);
                        this.onCloseGlaze(model);
                    } else {
                        Logger.log('Glaze: rendering', glaze);
                        Events.dispatch('glazeSelected', {
                            glaze_id: glaze.id,
                            campaign_id: glaze.campaign_id,
                            campaign_title: glaze.campaign_title,
                            glaze_title: glaze.title
                        });

                        var view = new GlazeView({
                            model: new Backbone.Model(glaze),
                            parent: this
                        });
                        var selectedVariant = this.preSelectGlazeVariant(glaze);
                        view.render().$el.appendTo(this.$el);
                        view.applySettings(ignoreConditions);
                        this.glazeViews.push(view);

                        if (!this.checkElementActionProducts(selectedVariant)) {
                            this.updatePreviewPreloader('Glaze: Action products are out of stock')
                        }
                    }
                }
            }, this));

            window.glaze.API('setActiveGlazes', this.glazeViews);
        },

        renderAllAvailableGlazes: function(exception_glaze_id) {
            if (this.renderedAllAvailableGlazes) return;
            this.renderedAllAvailableGlazes = true;
            _.each(this.availableGlazes, _.bind(function(glaze){
                if (exception_glaze_id != glaze.id) {
                    this.renderGlaze([glaze], false, true);
                }
            }, this));
        },

        updateDimensions: function() {
            if (this.$el.css('position') === 'fixed') { // CSS is loaded
                /*if ($(document).height() != this.docHeight || this.$('.glaze-layer').height() != this.docHeight) {
                    this.docHeight = $(document).height();
                    this.$('.glaze-layer').css('height', 0);
                    this.$('.glaze-layer').css('height', this.docHeight);
                }*/
                if (this.$('.glaze-layer').width() !== this.docWidth) {
                    this.docWidth = this.$('.glaze-layer').width();
                }

                _.each(this.glazeViews, _.bind(function(view){
                    if (view) {
                        view.updateGlazeDimensions($(window).width(), MiscUtils.getWindowHeight());
                        view.updateGlazePosition();
                    }
                }, this));
            }
        },

        selectGlaze: function() {
            if (!this.appData.get('glazes') || !this.appData.get('glazes').length) {
                return false;
            }

            _.each(this.appData.get('glazes'), _.bind(function(glaze){
                if (this.appData.get('previewGlaze') && glaze.hash === this.appData.get('previewGlaze')) {
                    glaze.preview = true;
                    MiscUtils.sessionStorage().setItem('preview_campaign', glaze.campaign_id);
                }
            }, this));

            var glazes = this.appData.getSortedGlazes(TrackingManager.getLastSeenCampaignId());

            glazes = _.filter(glazes, _.bind(function(glaze) {
                var selectedVariant = this.preSelectGlazeVariant(glaze),
                    campaignSettings = JSON.parse(glaze.campaign_settings || '{}'),
                    glazeSettings = (selectedVariant && JSON.parse(selectedVariant.settings || '{}')) || {};

                glaze.forceRestart = false;

                if (!campaignSettings.advanced_settings || JSON.stringify(campaignSettings.advanced_settings) === '{}') {
                    campaignSettings.advanced_settings = [];
                }
                if (glaze.campaign_conditions) {
                    campaignSettings.advanced_settings = campaignSettings.advanced_settings.concat(glaze.campaign_conditions || []);
                }

                glazeSettings = this.appData.extendSettings(campaignSettings, glazeSettings);

                if (!glazeSettings.advanced_settings || JSON.stringify(glazeSettings.advanced_settings) === '{}') {
                    glazeSettings.advanced_settings = [];
                }

                if (selectedVariant && selectedVariant.conditions && selectedVariant.conditions.length) {
                    glazeSettings.advanced_settings = glazeSettings.advanced_settings.concat(selectedVariant.conditions);
                }

                if (glazeSettings.restart
                    && glazeSettings.restart_appearance === 'every'
                    && TrackingManager.isCampaignClosed(glaze.campaign_id)
                ) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' force restart enabled');
                    glaze.forceRestart = true;
                }

                if (selectedVariant && selectedVariant.out_of_capping) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' reached frequency capping limit, skipping');
                    return false;
                }

                if (this.checkWebsiteImpressionsLimit(glaze)) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' reached website impressions limit, skipping');
                    return false;
                }

                if (this.checkWebsiteCampaignsLimit(glaze)) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' reached website campaigns limit, skipping');
                    return false;
                }

                if (!this.checkElementActionProducts(selectedVariant) && !glazeSettings.trigger_action_products_oos && !this.appData.get('previewGlaze')) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' did not pass elements action products check - out of stock, skipping');
                    return false;
                }

                this.currentCheckingGlaze = glaze;
                var passAdditionalSettings = this.passAdditionalSettings(glazeSettings.advanced_settings),
                    passAdditionalCampaignSettings = this.passAdditionalSettings(campaignSettings.advanced_settings);

                if (!passAdditionalSettings ||
                    !passAdditionalCampaignSettings ||
                    !this.passAdditionalSettings(this.appData.get('website').audiences) ||
                    !this.passCampaignSettings(glaze)) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' did not pass conditions, skipping');
                    return false;
                }

                glaze.isDeferred = false;
                if (passAdditionalSettings === 'deferred' || passAdditionalCampaignSettings === 'deferred') {
                    glaze.isDeferred = true;
                }

                glaze.isAsync = false;
                if (passAdditionalSettings === 'async' || passAdditionalCampaignSettings === 'async') {
                    glaze.isAsync = true
                }

                if (glaze.preview) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' preview');
                    this.availableGlazes.push(glaze);
                    return true;
                }

                if (glaze.previewByCampaign) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' preview by campaign');
                    this.availableGlazes.push(glaze);
                    return true;
                }

                this.availableGlazes.push(glaze);

                if (TrackingManager.isConverted(glaze.id, glaze.conversion_round) && this.abHasGlaze != 0) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' is already converted, skipping');
                    if (!glaze.forceRestart) return false;
                }

                if (glaze.campaign_test_id) {
                    var selected_campaign_id = TrackingManager.getSelectedCampaignInSplitTest(glaze.campaign_test_id);
                    if (selected_campaign_id && Number(selected_campaign_id) !== Number(glaze.campaign_id)) {
                        Logger.log('Glaze: glaze ' + glaze.title + ' skipped due to another campaign is selected in split test');
                        return false;
                    } else if (!selected_campaign_id && !this.checkSplitTraffic(glaze, campaignSettings, this.randomSeedTraffic)) {
                        Logger.log('Glaze: glaze ' + glaze.title + ' skipped due to campaign split traffic');
                        return false;
                    }
                }
                if (Number(TrackingManager.getLastSeenCampaignId()) === Number(glaze.campaign_id)) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' applied because of continuous campaign view');
                }

                return true;
            }, this));

            this.currentCheckingGlaze = null;
            return glazes;
        },

        checkElementActionProducts: function(glazeVariant) {
            var check = true;
            if (!glazeVariant) return check;
            _.each(glazeVariant.elements || [], function(element) {
                var settings = JSON.parse(element.settings || '{}')
                if (element.action) {
                    settings.actionType = element.action.type;
                }

                if (settings.actionType === 'js' && element.products) {
                    var productsAvailable = _.every(element.products, function(item) {
                        return !!item.availability
                    });
                    if (!productsAvailable) check = false;
                }
            });

            return check;
        },

        checkSplitTraffic: function(glaze, campaignSettings, random) {
            if (!campaignSettings.split_traffic) return true;

            var campaignTrafficBounds = [0, parseInt(campaignSettings.split_traffic) / 100];
            if (!this.campaignSplitTraffic[glaze.campaign_id]) {
                _.each(this.campaignSplitTraffic, function(item, key){
                    campaignTrafficBounds[0] += item[1] - item[0];
                    campaignTrafficBounds[1] += item[1] - item[0];
                });

                this.campaignSplitTraffic[glaze.campaign_id] = campaignTrafficBounds;
            } else {
                campaignTrafficBounds = this.campaignSplitTraffic[glaze.campaign_id];
            }

            return random >= campaignTrafficBounds[0] && random < campaignTrafficBounds[1];
        },

        preSelectGlazeVariant: function(glaze) {
            _.each(glaze.variants, _.bind(function(variant){
                if (variant.screen_size_id) {
                    variant.screen_size = this.appData.getScreenSizeById(variant.screen_size_id);
                }
            }, this));
            var variants = _.sortBy(glaze.variants, function(variant){
                    if (!variant.screen_size.max || variant.screen_size.max == 0) return 1000000;
                    return Number(variant.screen_size.max);
                }),
                variantsByMin = _.sortBy(glaze.variants, function(variant){
                    return variant.screen_size.min;
                }),
                defaultVariant = _.find(glaze.variants, function(variant){ return variant.screen_size.is_default == 1 || !variant.screen_size.max || variant.screen_size.max == 0; }),
                windowWidth = $(window).width(),
                selectedVariant;

            selectedVariant = _.find(variants, function(variant){
                var max = variant.screen_size.max && variant.screen_size.max != 0 ? variant.screen_size.max : 1000000;

                return Number(max) >= windowWidth;
            });
            if (!selectedVariant) {
                if (glaze.campaign_is_mobile_first == 1 && variantsByMin && variantsByMin.length) {
                    selectedVariant = variantsByMin[0];
                } else {
                    selectedVariant = defaultVariant;
                }
            }
            if (!selectedVariant && variantsByMin && variantsByMin.length) {
                selectedVariant = variantsByMin[0];
            }

            return selectedVariant;
        },

        openGlazeById: function(id, ignoreConditions) {
            ignoreConditions = ignoreConditions || false;
            var glaze = _.find(this.appData.get('glazes'), function(glaze) {
                return glaze.id == parseInt(id, 10);
            });
            if (!ignoreConditions) {
                var selectedVariant = this.preSelectGlazeVariant(glaze),
                    campaignSettings = JSON.parse(glaze.campaign_settings),
                    glazeSettings = (selectedVariant && JSON.parse(selectedVariant.settings || '{}')) || {};

                if (!campaignSettings.advanced_settings || JSON.stringify(campaignSettings.advanced_settings) === '{}') {
                    campaignSettings.advanced_settings = [];
                }
                if (glaze.campaign_conditions) {
                    campaignSettings.advanced_settings = campaignSettings.advanced_settings.concat(glaze.campaign_conditions || []);
                }

                if (!glazeSettings.advanced_settings || JSON.stringify(glazeSettings.advanced_settings) === '{}') {
                    glazeSettings.advanced_settings = [];
                }
                glazeSettings = this.appData.extendSettings(campaignSettings, glazeSettings);

                if (selectedVariant && selectedVariant.conditions && selectedVariant.conditions.length) {
                    glazeSettings.advanced_settings = glazeSettings.advanced_settings.concat(selectedVariant.conditions);
                }
                if (!this.passAdditionalSettings(glazeSettings.advanced_settings) ||
                    !this.passCampaignSettings(glaze)) {
                    Logger.log('Glaze: glaze ' + glaze.title + ' did not pass conditions, skipping');
                    return false;
                }
            }
            this.renderGlaze([glaze], ignoreConditions);
            this.docHeight = null;
            this.updateDimensions();
        },

        checkWebsiteImpressionsLimit: function(glaze) {
            let website = this.appData.get('website')
            let impressionsAmount = TrackingManager.getImpressionsNumberTotal('session');
            return (website.is_impressions_limit_enabled && impressionsAmount >= website.impressions_limit);
        },

        checkWebsiteCampaignsLimit: function(glaze) {
            let website = this.appData.get('website')
            let campaignsAmount = TrackingManager.getCampaignsTotal('session', glaze.campaign_id);
            return (website.is_campaigns_limit_enabled && campaignsAmount >= website.campaigns_limit);
        },

        passCampaignSettings: function(glaze) {
            var campaignSettings = JSON.parse(glaze.campaign_settings) || {};
            if (campaignSettings.max_glazes) {
                var max = parseInt(campaignSettings.max_glazes),
                    numberOfGlazesVisited = Number(TrackingManager.getViewedGlazesNumber(this.model.get('campaign_id')) || 0);

                if (numberOfGlazesVisited >= max) {
                    return false;
                }
            }

            if (!glaze.preview &&
                campaignSettings.freq_capping_cmp &&
                campaignSettings.freq_capping_cmp_times &&
                (TrackingManager.getImpressionsNumber(glaze.id) >= parseInt(campaignSettings.freq_capping_cmp_times) /*||
                TrackingManager.getCampaignImpressions(glaze.campaign_id) >= parseInt(campaignSettings.freq_capping_cmp_times)*/)) {

                Logger.log('Glaze: glaze ' + glaze.title + ' did not pass campaign level frequency capping, skipping');
                return false;
            }
            return true;
        },

        passAdditionalSettings: function(conditions) {
            if (!conditions) return true;

            var passType = _.map(conditions, _.bind(function(item) {
                return this.passConditionGroup(item);
            }, this));

            var pass = _.all(passType, function(item) { return !!item; });
            if (pass && passType.length && _.all(passType, function(item) {
                return item === 'deferred';
            })) pass = 'deferred';
            if (pass && passType.length && _.some(passType, function(item) {
                return item === 'async';
            })) pass = 'async';

            return pass;
        },

        passConditionGroup: function(condition) {
            _.each(condition.code, function(conditionGroup, groupKey) {
                if (groupKey) {
                    _.each(conditionGroup, function(conditionItem, key) {
                        if (key) {
                            if (condition.products && conditionItem.type === 'custom-js' && conditionItem.name.match('{PRODUCT')) {
                                conditionItem.products = {
                                    products: condition.products,
                                    categories:  condition.categories
                                };
                            }
                            if (conditionItem.type === 'custom-js-products') {
                                conditionItem.id = condition.id;
                                conditionItem.glaze_id = condition.glaze_id;
                                conditionItem.glaze_template_variant_id = condition.glaze_template_variant_id;
                                conditionItem.condition_template_id = condition.condition_template_id;
                            }
                            if (condition.value && conditionItem.type === 'ad') {
                                conditionItem.value = condition.value;
                            }
                            if (conditionItem.type === 'selector-available') {
                                conditionItem.page_template_selectors = condition.page_template_selectors;
                            }
                            if (condition.dynamic_field && conditionItem.type === 'custom-js') {
                                conditionItem.name = (conditionItem.name || '').replace(/{DFIELD:[^}]+\}/, condition.dynamic_field);
                            }
                        }
                    });
                }
            });

            var passType = _.map(condition.code, _.bind(function(item, index) {
                return this.passConditionSubGroup(item, index);
            }, this));
            var pass = _.all(passType, function(item) { return !!item; });

            if (pass && _.some(passType, function(item) {
                return item === 'async';
            })) pass = 'async';
            if (pass !== 'async') {
                pass = condition.action && Number(condition.action) === 1 ? pass : !pass;
            }

            if (pass && _.some(passType, function(item) {
                return item === 'deferred';
            })) pass = 'deferred';

            if (!pass) {
                Logger.log('Glaze: glaze did not pass condition ' + condition.title);
            }

            return pass;
        },

        passConditionSubGroup: function(condition, key) {
            if (!key) return true;

            var passType = _.map(condition, _.bind(function(subitem, subindex) {
                return this.passCondition(subitem, subindex);
            }, this));
            var pass = _.some(passType, function(item) { return !!item; });

            if (pass && _.some(passType, function(item) {
                return item === 'deferred';
            })) pass = 'deferred';
            if (pass && _.some(passType, function(item) {
                return item === 'async';
            })) pass = 'async';

            return pass;
        },

        passCondition: function(condition, key) {
            if (!key) return false;
            return ConditionChecker.checkCondition(condition);
        },

        exitIntent: function(event) {
            if (!this.glazeViews.length) return;
            _.each(this.glazeViews, function(view){
                view && view.exitIntent(event);
            });
        },

        mouseOut: function(event) {
            if (!this.glazeViews.length) return;
            _.each(this.glazeViews, function(view){
                view && view.rollOn(event);
            });
        },

        mouseOver: function(event) {
            if (!this.glazeViews.length) return;
            _.each(this.glazeViews, function(view){
                if (view) {
                    view.contentIntent(event);
                    view.rollOn(event);
                }
            });
        },

        clickShape: function(event) {
            TrackingManager.checkClickElement(event);
            var activeGlazes = [];
            _.each(this.glazeViews, function(view){
                if (view) {
                    view.clickShape(event);
                    activeGlazes.push(view.model.get('id'));
                }
            });
            TrackingManager.checkVoidVTC(activeGlazes, event);
        },

        onTransition: function(event) {
            _.each(this.glazeViews, function(view){
                if (view) {
                    view.updateGlazePosition();
                    view.checkVisibility();
                }
            });
        },

        updatePositionOnScroll: function() {
            _.each(this.glazeViews, function(view){
                view && view.updateGlazePosition();
            });
        },

        scrollOrResize: function(event) {
            if (this.checkVisibilityTimeout) {
                clearTimeout(this.checkVisibilityTimeout);
                this.checkVisibilityTimeout = null;
            }
            this.checkVisibilityTimeout = setTimeout(_.bind(function(){
                _.each(this.glazeViews, function(view){
                    view && view.checkVisibility();
                });
            }, this), 50);
        },

        keyUp: function(event) {
            if (event.keyCode === 27) {
                _.each(this.glazeViews, function(view){
                    Logger.log('Glaze: closing by key up');
                    view && view.hideGlaze();
                });
            }
            if (event.keyCode === 9) {
                if (this.glazeViews.length && !$(event.target).parents('.glaze-view').length) {
                    var $tabbable = this.glazeViews[0].$('button, [href], input, select, textarea').filter(':visible');
                    if (!event.shiftKey && $tabbable.length) {
                        $tabbable.first().focus();
                    } else if (event.shiftKey && $tabbable.length) {
                        $tabbable.last().focus();
                    }
                }
            }
        },

        /*DOMMutate: function(event) {
            // this.updateDimensions();
            // _.each(this.glazeViews, function(view){
            //     view && view.checkBindedElement();
            // });
        },*/

        onCloseGlaze: function(model, converted) {
            converted = converted || false;
            this.lastGlaze = model;
            var settings = this.appData.extendSettings(JSON.parse(this.lastGlaze.get('campaign_settings')), this.lastGlaze.get('variant').get('settings')),
                $restart = this.$('.glaze-restart');

            var glazeView = _.find(this.glazeViews, _.bind(function(view){
                return view && view.model.get('id') == this.lastGlaze.get('id');
            }, this));

            if (glazeView) {
                var abHasGlaze = glazeView.abHasGlaze;
                this.glazeViews.splice(this.glazeViews.indexOf(glazeView), 1);
            }

            if (settings.restart_appearance && settings.restart_appearance === 'every' && this.lastGlaze) {
                TrackingManager.setRestartCampaign(this.lastGlaze.get('campaign_id'));
            }

            if (settings && settings.restart) {
                if (settings.restart_image) {
                    $restart.css('backgroundImage', 'url(' + settings.restart_image + ')');
                }
                setTimeout(_.bind(function(){
                    $restart.html(this.getRestartText())
                        .show()
                        .attr('aria-hidden', false)
                        .focus();
                }, this), 0);

                if (settings.restart_text_color) {
                    $restart.css('color', settings.restart_text_color);
                }
                if (settings.restart_tab_color) {
                    $restart.css('background', settings.restart_tab_color);
                }
                if (settings.restart_position && settings.restart_position !== 'default') {
                    switch (settings.restart_position) {
                        case 'bl': $restart.css({top: 'auto', bottom: 0, left: 0, right: 'auto'}); break;
                        case 'tl': $restart.css({top: 0, bottom: 'auto', left: 0, right: 'auto'}); break;
                        case 'tr': $restart.css({top: 0, bottom: 'auto', left: 'auto', right: 0}); break;
                    }
                }
                if (settings.restart_text_font) {
                    if (_.isObject(settings.restart_text_font)) {
                        $restart.css({fontFamily: settings.restart_text_font.family});
                    } else {
                        $restart.css('fontFamily', settings.restart_text_font);
                    }

                    if (settings.restart_text_font.customId) {
                        FontLoader.loadCustomFont(settings.restart_text_font.customId);
                    } else {
                        FontLoader.loadFonts([settings.restart_text_font.family]);
                    }
                }
                if (settings.restart_text_size) {
                    $restart.css('fontSize', settings.restart_text_size + 'px');
                }
            }

            if (glazeView) {
                if (settings.close_action_type && (!settings.close_action_condition || (settings.close_action_condition === 'not_convert' && !converted) || (settings.close_action_condition === 'convert' && converted))) {
                    if (settings.close_action_type === 'url') {
                        if (settings.close_action_url_blank) {
                            var win = window.open(settings.close_action_url, '_blank');
                            win.focus();
                        } else {
                            window.location.href = settings.close_action_url;
                        }
                    } else if (settings.close_action_type === 'glaze') {
