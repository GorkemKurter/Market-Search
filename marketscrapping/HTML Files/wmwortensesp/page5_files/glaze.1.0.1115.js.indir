                        {placeholder: '{PROMOTED_PRODUCT.NAME}', value: (product.name || '').trim()},
                        {placeholder: '{PROMOTED_PRODUCT.VENDOR}', value: (product.vendor || '').trim()},
                        {placeholder: '{PROMOTED_PRODUCT.CHANNEL_ID}', value: (product.custom_data && product.custom_data.channel_id || '').trim()},
                        {placeholder: '{PROMOTED_PRODUCT.OFFER_ID}', value: (product.custom_data && product.custom_data.offer_id || '').trim()},
                        {placeholder: '{PROMOTED_PRODUCT.SOURCE_ID}', value: (product.custom_data && product.custom_data.source_id || '').trim()},
