    var _ = require('underscore'),
        Settings = require('Settings'),
        WebFont = require('webfontloader');

    var FontLoader = {
        loadedFonts: {
            common: [],
            custom: []
        },
        loadFonts: function(fonts, cb) {
            fonts = fonts || [];
            if(!_.isArray(fonts)) {
                fonts = [fonts];
            }
            fonts = _.difference(fonts, Settings.excludedFonts);
            fonts = _.difference(fonts, this.loadedFonts.common);
            fonts = _.filter(fonts, function(item) {
                return !!item;
            });

            if(fonts.length) {
                WebFont.load({
                    google: {
                        families: fonts
                    }
                });
                _.each(fonts, _.bind(function(font) {
                    this.loadedFonts.common.push(font);
                }, this))
            }
        },
        loadCustomFont: function(font) {
            if(this.loadedFonts.custom.indexOf(font) === -1) {
                if(!Settings.cdnHost) {
                    $('head').append('<link rel="stylesheet" type="text/css" href="' + Settings.apiHost + '/api/fonts/' + font + '">')
                } else {
                    var linkPrefix = 'fonts';
                    if(Settings.environment !== 'prod' && Settings.environment !== 'production') {
                        linkPrefix = Settings.environment + '-' + linkPrefix;
                    }
                    $('head').append('<link rel="stylesheet" type="text/css" href="' + Settings.cdnHost + '/' + linkPrefix + '/font-' + font + '.css">')
                    this.loadedFonts.custom.push(font);
                }
            }
        }
    };

    return FontLoader;
