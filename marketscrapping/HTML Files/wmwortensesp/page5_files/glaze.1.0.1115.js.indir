                    $elementSize = $item[0].getBoundingClientRect(),
                    fixedPosition = false;

                $elementSize['height'] = $elementSize.bottom - $elementSize.top;

                $item.parents().each(function(){
                    if ($(this).css('position') === 'fixed') {
                        fixedPosition = true;
                    }
                });

                if ($item.is(':visible') && $elementSize.width > 0 && $elementSize.height > 0) {
                    dimensions.left = _.min([dimensions.left, $item.offset().left]);
                    dimensions.top = _.min([dimensions.top, $elementSize.top]);
                    if (fixedPosition) {
                        dimensions.top += $('body').scrollTop();
                    }
                    dimensions.right = _.max([dimensions.right, $item.offset().left + $elementSize.width]);
                    dimensions.bottom = _.max([dimensions.bottom, $elementSize.top + $elementSize.height, dimensions.top + $elementSize.height]);
                }
            });

            dimensions.width = dimensions.right - dimensions.left;
            dimensions.height = dimensions.bottom - dimensions.top;

            delete dimensions.bottom;
            delete dimensions.right;

            return dimensions;
        },

        updateGlazeElementsPosition: function() {
            _.each(this.elements, function(element){
                element.updatePosition();
            });
        },

        applySettings: function(ignoreConditions) {
            ignoreConditions = ignoreConditions || false;
            var settings = this._getSettings();

            if (!ignoreConditions) {
                if (!settings.advanced_settings || JSON.stringify(settings.advanced_settings) === '{}') {
                    settings.advanced_settings = [];
                }
                settings.advanced_settings = settings.advanced_settings.concat(this.model.get('variant').get('conditions') || []);
                this._setDeferredConditions(settings.advanced_settings);
            }

            if (!ignoreConditions) {
                if (settings.content_intent && !settings.content_intent_selector) {
                    Logger.log('Glaze: glaze ' + this.model.get('title') + ' content intent is set, but content intent selector is empty, ignoring');
                    settings.content_intent = false;
                }
                if (!settings.exit_intent && !settings.content_intent && !settings.roll_on) {
                    this.showGlazeTimeout = setTimeout(_.bind(this.showGlaze, this), settings.trigger_timer && settings.timer_in ? settings.timer_in : 0);
                } else if ((settings.exit_intent || settings.content_intent) && settings.trigger_timer && settings.timer_in) {
                    this.showGlazeTimeout = setTimeout(_.bind(function(){
                        if (!this.isShown) {
                            this.showGlaze();
                        }
                    }, this), settings.timer_in);
                }
            } else {
                if (!this.isShown) {
                    this.showGlaze(ignoreConditions);
                }
            }
        },

        showGlaze: function(ignoreConditions) {
            ignoreConditions = ignoreConditions || this.ingoreConditionsOnShow;
            this.ingoreConditionsOnShow = false;

            var settings = this._getSettings(),
                forceAutoscroll = false;

            if (!this.canvasReady) {
                this.initCanvas();
                setTimeout(_.bind(function(){
                    this.showGlaze(ignoreConditions);
                }, this), 300);
                return;
            }

            if (this.parent.activeGlaze) {
                this.isReadyToShow = true;
                return;
            }
            
            if (this.showGlazeTimeout) {
                clearTimeout(this.showGlazeTimeout);
                this.showGlazeTimeout = null;
            }
 
            var glazeSettings = this._getSettings(),
                campaignSettings = JSON.parse(this.model.get('campaign_settings') || '{}');

            if (!ignoreConditions) {
                if (!this.parent.passAdditionalSettings(glazeSettings.advanced_settings) ||
                    !this.parent.passAdditionalSettings(campaignSettings.advanced_settings || []) ||
                    !this.parent.passCampaignSettings(this.model.toJSON())) {
                    Logger.log('Glaze: glaze ' + this.model.get('title') + ' did not pass conditions on glaze show');
                    return false;
                }
            }

            if (this.deferredConditions.length) {
                if (!this.deferredConditionsRunning) {
                    this._startCheckingDeferredConditions();
                }
                Logger.log('Glaze: ready to show, waiting for deferred conditions to complete');
                return;
            }

            if (!this.isElementCreated()) {
                if (!this.waitingForElementInterval) {
                    Logger.log('Glaze: ready to show, binded element not found or not created yet');
                    TrackingManager.notifyNoElement(
                        this.model.get('id'),
                        this.model.get('variant').getSelector(0)
                    );

                    if (settings.out_viewport_action_type === 'glaze' && settings.out_viewport_action_glaze) {
                        Logger.log('Glaze: open glaze by action if binded element is not found');
                        this.undelegateResizeListener();
                        this.isShown = false;
                        $('html,body').stop();
                        $('body').removeClass('glaze-effect');
                        this.removeAdditionalCss();
                        this.remove();
                        this.parent.openGlazeById(settings.out_viewport_action_glaze, false);
                        return;
                    } else {
                        this.isReadyToShow = true;
                        this.waitingForElementInterval = setInterval(_.bind(this.showGlaze, this), 400);
                    }
                } else {
                    Logger.log('Glaze: Waiting for element to be created');
                }
                return;
            } else {
                clearInterval(this.waitingForElementInterval);
                this.waitingForElementInterval = null;
            }

            if (this.parent.restartingGlaze == this.model.get('id')) {
                settings.autoscroll = true;
                forceAutoscroll = true;
                this.parent.restartingGlaze = null;
            }

            if (!this.isShapeVisible()) {
                Logger.log('Glaze: ready to show, shape is out of the viewport');
                this.isReadyToShow = true;
                this.ingoreConditionsOnShow = ignoreConditions;
                if (settings.autoscroll || settings.exit_intent) {
                    this.autoscroll(undefined, forceAutoscroll);
                } else {
                    if (!this.waitingForViewportElementInteval) {
                        this.waitingForViewportElementInteval = setInterval(_.bind(this.showGlaze, this), 400);
                    }
                }
                return;
            } else {
                if (this.waitingForViewportElementInteval) {
                    clearInterval(this.waitingForViewportElementInteval);
                    this.waitingForViewportElementInteval = null;
                }
            }

            this.selectABVersion();

            this.isReadyToShow = false;
            if ((settings.autoscroll || settings.exit_intent) && !this.autoscrollDone) {
                this.autoscroll(undefined, forceAutoscroll);
            }

            Logger.log('Glaze: show');
            this.parent.hideRestart();
            this.executePreJS();
            this.beforeShow();
            this.applyAdditionalCss();
            this.parent.updateDimensions();
            this.setActiveGlaze(this.model.get('id'));
            this.animateIn(settings.animate_in, function() {
                setTimeout(_.bind(this.afterShow, this), 300);
                if (settings.close_timer && settings.timer_out) {
                    setTimeout(_.bind(function() {
                        Logger.log('Glaze: closing by timer ' + settings.timer_out)
                        _.bind(this.hideGlaze, this);
                    }, this), settings.timer_out);
                }
            });

            this.$el.parent().focus();
        },

        setActiveGlaze: function(value) {
            this.parent.activeGlaze = value;
        },

        applyAdditionalCss: function() {
            var settings = this._getSettings(),
                websiteCss = this.appData.get('website').additional_css;

            if (websiteCss) {
                $('<style type="text/css" id="glazeWebsiteStyles">' + websiteCss + '</style>').appendTo('head');
            }

            if (settings.campaign_css_rules) {
                $('<style type="text/css" id="glazeCampaignStyles">' + settings.campaign_css_rules + '</style>').appendTo('head');
            }

            if (settings.css_rules) {
                $('<style type="text/css" id="glazeStyles">' + settings.css_rules + '</style>').appendTo('head');
            }
        },

        removeAdditionalCss: function() {
            $('head').find('#glazeStyles').remove();
            $('head').find('#glazeCampaignStyles').remove();
            $('head').find('#glazeWebsiteStyles').remove();
        },

        selectABVersion: function() {
            var percent = (this._getSettings().show_percent || 100) / 100,
                chance = this.parent.randomSeedAB,
                savedABVersion = this.parent.abHasGlaze;

            if (savedABVersion != -1) {
                this.abHasGlaze = savedABVersion;
            } else {
                this.abHasGlaze = (percent >= chance) ? 1 : 0;
                Logger.log('Glaze: version without glaze percent: ' + this._getSettings().show_percent);
                TrackingManager.setAB(this.model.get('campaign_id'), this.abHasGlaze);
                this.parent.abHasGlaze = this.abHasGlaze;
            }

            if (this.abHasGlaze == 0 && !this.appData.get('previewGlaze')) {
                Logger.log('Glaze: A/B testing, version without glaze');
                this.$el.css({visibility: 'hidden', pointerEvents: 'none'});
                _.each(this.elements, function(element){
                    element.$el.css({visibility: 'hidden', pointerEvents: 'none'});
                });
                this.parent.renderAllAvailableGlazes(this.model.get('id'));
            } else {
                Logger.log('Glaze: A/B testing, version with glaze');
            }
        },

        actionOnGlazeOpen: function() {
            var settings = this._getSettings();
            // if (!this.abHasGlaze) return;

            var actionCode = settings.open_action_js;
            if (actionCode) {
                actionCode = actionCode.replace(/GLAZE_ID/g, "'" + this.model.get('id') + "'");
                actionCode = actionCode.replace(/GLAZE_TITLE/g, "'" + this.model.get('title') + "'");
                actionCode = actionCode.replace(/CAMPAIGN_ID/g, "'" + this.model.get('campaign_id') + "'");
                actionCode = actionCode.replace(/[‘’]+/g, "'");
                Logger.log('Glaze: execute action on glaze open ' + actionCode);
                try {
                    eval(actionCode);
                } catch(err) {
                    console.warn('Error in Action on Glaze Open');
                    console.warn(err);
                }
            }

            var singleTagAction = settings.universal_action_js;
            if (singleTagAction) {
                singleTagAction = MiscUtils.prepareActionCode(
                    singleTagAction,
                    'GLAZE_OPEN',
                    this.model.get('id'),
                    this.model.get('title'),
                    this.model.get('campaign_id'),
                    this.model.get('campaign_title'),
                    this.abHasGlaze,
                    this.model.get('variant').getSelector(0)
                );

                Logger.log('Glaze: execute single tag action on glaze open ' + singleTagAction);
                try {
                    eval(singleTagAction);
                } catch(err) {
                    console.warn('Error in Single Tag Action on Glaze Open');
                    console.warn(err);
                }
            }

            Events.dispatch('glazeActivated', {
                glaze_id: this.model.get('id'),
                campaign_id: this.model.get('campaign_id'),
                campaign_title: this.model.get('campaign_title'),
                glaze_title: this.model.get('title'),
                glaze_visible: !!this.abHasGlaze ? 'yes' : 'no'
            });
        },

        executePreJS: function() {
            if (this.appData.get('website').prejs) {
                eval(this.appData.get('website').prejs);
            }
        },

        executePostJS: function() {
            if (this.appData.get('website').postjs) {
                eval(this.appData.get('website').postjs);
            }
        },

        animateIn: function(animation, cb) {
            var animation = animation && animation !== 'null' ? animation : 'none',
                settings = this._getSettings();

            if (settings.z_index) {
                this.parent.$el.css('zIndex', settings.z_index);
            }

            Logger.log('Glaze: animate in - ' + animation);
            switch(animation) {
                case 'Dissolve':
                    this.$el.fadeIn(700, _.bind(cb, this));
                    break;
                case 'Move left to right':
                    this.$el.css({left: '-100%', display: 'block'}).animate({left: 0}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move right to left':
                    this.$el.css({left: '100%', display: 'block'}).animate({left: 0}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move top to bottom':
                    this.$el.css({top: '-100%', display: 'block'}).animate({top: 0}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move bottom to top':
                    this.$el.css({top: '100%', display: 'block'}).animate({top: 0}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move top left to bottom right':
                    this.$el.css({top: '-100%', left: '-100%', display: 'block'}).animate({top: 0, left: 0}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move top right to bottom left':
                    this.$el.css({top: '-100%', left: '100%', display: 'block'}).animate({top: 0, left: 0}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move bottom left to top right':
                    this.$el.css({top: '100%', left: '-100%', display: 'block'}).animate({top: 0, left: 0}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move bottom right to top left':
                    this.$el.css({top: '100%', left: '100%', display: 'block'}).animate({top: 0, left: 0}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'none':
                    this.$el.css({display: 'block'});
                    cb.call(this);
                    break;
                default:
                    this.$el.show(animation.toLowerCase(), 1000, _.bind(cb, this));
            }
        },

        animateOut: function(animation, cb) {
            var animation = animation && animation !== 'null' ? animation : 'none';

            this.parent.$el.css('zIndex', 9000);

            Logger.log('Glaze: animate out - ' + animation);
            switch(animation) {
                case 'Dissolve':
                    this.$el.fadeOut(700, _.bind(cb, this));
                    break;
                case 'Move left to right':
                    this.$el.css({left: 0, display: 'block'}).animate({left: '100%'}, 1000, 'easeOutCirc', _.bind(cb, this));
                    break;
                case 'Move right to left':
                    this.$el.css({left: 0, display: 'block'}).animate({left: '-100%'}, 1000, 'easeOutCirc', _.bind(cb, this));
                    break;
                case 'Move top to bottom':
                    this.$el.css({top: 0, display: 'block'}).animate({top: '100%'}, 1000, 'easeOutCirc', _.bind(cb, this));
                    break;
                case 'Move bottom to top':
                    this.$el.css({top: 0, display: 'block'}).animate({top: '-100%'}, 1000, 'easeOutCirc', _.bind(cb, this));
                    break;
                case 'Move top left to bottom right':
                    this.$el.css({top: 0, left: 0, display: 'block'}).animate({top: '100%', left: '100%'}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move top right to bottom left':
                    this.$el.css({top: 0, left: 0, display: 'block'}).animate({top: '100%', left: '-100%'}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move bottom left to top right':
                    this.$el.css({top: 0, left: 0, display: 'block'}).animate({top: '-100%', left: '100%'}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'Move bottom right to top left':
                    this.$el.css({top: 0, left: 0, display: 'block'}).animate({top: '-100%', left: '-100%'}, 1000, 'easeOutCirc',  _.bind(cb, this));
                    break;
                case 'none':
                    this.$el.css({display: 'none'}) && cb.call(this);
                    break;
                default:
                    this.$el.hide(animation.toLowerCase(), 1000, _.bind(cb, this));
            }
        },

        isElementPositionFixed: function($element) {
            if (!$element) return false;
            var fixedPosition = $element.css('position') === 'fixed';

            $element.parents(':not("html,body")').each(function() {
                if ($(this).css('position') === 'fixed') {
                    fixedPosition = true;
                }
            });

            return fixedPosition;
        },

        autoscroll: function(topValue, forceAutoscroll, smoothTransition) {
            forceAutoscroll = forceAutoscroll || false;
            smoothTransition = smoothTransition || false;
            var glazeSettings = this._getSettings();

            if (this.abHasGlaze == 0) {
                return;
            }

            if (forceAutoscroll || (!this.autoscrollStarted && !glazeSettings.roll_on && (glazeSettings.autoscroll || glazeSettings.exit_intent ))) {
                var shape = this.canvas.item(0),
                    $element = null;

                if (glazeSettings.autoscroll_element_enable && (glazeSettings.autoscroll_element || glazeSettings.autoscroll_element_selector)) {
                    $element = $(document).find(glazeSettings.autoscroll_element_selector);
                }
                if (shape || $element) {
                    var container = this.appData.get('website').scroll_container || 'html,body',
                        containerOffsetTop = ($(container).offset() && $(container).offset().top) || 0;

                    if (this.model.get('variant').getSelector(0) && !$element) {
                        var $bindedElement = this.model.get('variant').getSpotlightedElement(0);
                        if (!$bindedElement.length || !$bindedElement.is(':visible')) {
                            Logger.log('Glaze: binded element not created or not visible, autoscroll impossible');
                            return;
                        }

                        if (this.isElementPositionFixed($bindedElement)) {
                            Logger.log('Glaze: binded element has fixed position, autoscroll not needed');
                            return;
                        }
                    }

                    if (!$element) {
                        var shapeBoundingRect = shape.getBoundingRect();
                        if (shapeBoundingRect.width > 10000000) {
                            setTimeout(_.bind(function(){
                                this.autoscroll(topValue, forceAutoscroll);
                            }, this), 300);
                            return;
                        }
                        if ($bindedElement && $bindedElement.length) {
                            topValue = _.isUndefined(topValue) ? $bindedElement.offset().top : topValue;
                        }
                    } else {
                        topValue = _.isUndefined(topValue) ? $element.offset().top : topValue;
                    }

                    var autoScrollSpeed = Number(glazeSettings.autoscroll_speed || Settings.glazeAutoScrollSpeed),
                        autoScrollOffset = glazeSettings.autoscroll_offset || Settings.glazeAutoScrollOffset;

                    if (glazeSettings.exit_intent) {
                        autoScrollOffset = glazeSettings.exit_autoscroll_offset;
                    }

                    var windowHeight = MiscUtils.getWindowHeight();
                    var top = parseInt(topValue - containerOffsetTop - parseInt(windowHeight * (autoScrollOffset / 100)));

                    container = container && container.toLowerCase();
                    if (container && container !== 'html,body' && container !== 'html' && container !== 'body') {
                        top += $(container).scrollTop();
                    }
                    top = top > 0 ? top : 0;

                    var scrollDiff = top - $(container).scrollTop();

                    if ($(container)[0].scrollHeight < windowHeight) {
                        top = 0;
                        scrollDiff = 0;
                    }

                    if (top >= 0) {
                        this.autoscrollStarted = true;
                        this.autoscrollDone = false;
                        if (!autoScrollSpeed && smoothTransition) {
                            autoScrollSpeed = 1000;
                        }
                        Logger.log('Glaze: autoscroll to the void: (' + autoScrollSpeed  + 'ms)');
                        $(container).animate({
                            scrollTop: top
                        }, autoScrollSpeed, _.bind(function(){
                            this.autoscrollStarted = false;
                            this.autoscrollDone = true;
                            this.updateGlazePosition();
                        }, this));


                        return scrollDiff;
                    }
                }
            }
        },

        beforeShow: function() {
            var variant = this.model.get('variant');
            if (variant.get('blur') != 0 || variant.get('grayscale') != 0) {
                var filter = [];

                variant.get('grayscale') && filter.push('grayscale(' + variant.get('grayscale') + '%)');
                variant.get('blur') && filter.push('blur(' + variant.get('blur') + 'px)');

                var $element = this.model.get('variant').getSpotlightedElement(0);

                if ($element) {
                    $element.addClass('glaze-exclude').addClass('glaze-bind-element');
                    $element.children().addClass('glaze-exclude');
                    $element.parents().addClass('glaze-exclude');

                    var style = '<style type="text/css">' +
                        'body.glaze-effect :not(.glaze-exclude){' +
                        '-webkit-filter: ' + filter.join(' ') + ';' +
                        '-moz-filter: ' + filter.join(' ') + ';' +
                        'filter: ' + filter.join(' ') + ';}' +
                        '</style>';

                    $('head').append(style);
                    $('body').addClass('glaze-effect');
                }
            }

            this.applyPreAccessibility();
        },

        afterShow: function() {
            var glazeSettings = this._getSettings();

            if (!this.canvasReady) {
                setTimeout(_.bind(this.afterShow, this), 300);
                return;
            }

            this.parent.updateDimensions();

            if (glazeSettings.deactivate_links) {
                this.$el.css('pointerEvents', 'all');
            } else {
                this.$el.css('pointerEvents', 'none');
            }

            this.isShown = true;
            this.applyPostAccessibility();
            this.delegateResizeListener();
            this.trackImpression();
            this.initGlazeElements();
            this.actionOnGlazeOpen();
        },

        applyPreAccessibility: function() {
            var ariaLabeledBy = 'title-trick-' + Date.now();
            this.$el.find('[id="' + this.attributes['aria-labelledby'] + '"]').attr('id', ariaLabeledBy);

            if (this.$('#glazeLabel').length) {
                this.$el.attr('aria-labelledby', 'glazeLabel');
            } else {
                this.attributes['aria-labelledby'] = ariaLabeledBy;
                this.$el.attr('aria-labelledby', ariaLabeledBy);
            }
        },

        applyPostAccessibility: function() {
            var glazeSettings = this._getSettings();

            this.$el.focus();

            if (this.model.get('variant').get('shape_id') !== -1) {
                this.$('.glaze-shape-button')[0].onclick = _.bind(this.clickVoid, this);
            }

            this.$('.glaze-shape-button span').html(glazeSettings.a11y_shape_btn_text ||  "Enregistrer l'offre d'emploi dans MEMO (Ouvre une fenêtre popup)")
        },

        initGlazeElements: function() {
            _.each(this.elements, function(element){
                element.start();
            });
        },

        delegateResizeListener: function() {
            window.requestAnimationFrame  = window.requestAnimationFrame || window.mozRequestAnimationFrame ||
                window.webkitRequestAnimationFrame || window.msRequestAnimationFrame;

            this.updateFrame = window.requestAnimationFrame(_.bind(this.checkDimensionChanges, this));
        },

        checkDimensionChanges: function() {
            var dimensions = [];
            _.each(this.model.get('variant').get('spotlightedElement'), _.bind(function($element){
                var rawDimensions = this.calculateSpotlightArea($element);
                _.each(rawDimensions, function(dimension, key) {
                    rawDimensions[key] = Math.round(dimension);
                });
                dimensions.push(rawDimensions);
            }, this));

            if (this.savedDimensions !== JSON.stringify(dimensions)) {
                this.savedDimensions = JSON.stringify(dimensions);
                this.updateGlazePosition();
            }
            if (this.isShown) {
                this.updateFrame = window.requestAnimationFrame(_.bind(this.checkDimensionChanges, this));
            }
        },

        undelegateResizeListener: function() {
            var cancelAnimationFrame = window.cancelAnimationFrame || window.mozCancelAnimationFrame || window.webkitCancelAnimationFrame ||
                window.clearTimeout;
            cancelAnimationFrame(this.updateFrame);
        },

        hideGlaze: function(cb, converted, options) {
            converted = converted || false;
            options = options || {};
            var settings = this._getSettings();

            if (!this.isShown) return;

            this.setActiveGlaze(null);
            if (settings.close_action_type &&
                (!settings.close_action_condition || (settings.close_action_condition === 'not_convert' && !converted) || (settings.close_action_condition === 'convert' && converted))) {
                if (settings.close_action_type === 'glaze' && !options.forceClose) {
                    if (settings.close_action_glaze_transition && settings.close_action_glaze_transition === 'smooth') {
                        this.removeAdditionalCss();
                        this.smoothTransitionGlaze(settings.close_action_glaze);
                        this.trackAbandon();
                        return;
                    }
                }
                if (settings.close_action_type === 'conditional' && !options.forceClose) {
                    this.removeAdditionalCss();
                    var isSuccess = eval(settings.close_action_condition_js);
                    this.trackAbandon();
                    if (isSuccess) return;
                }
            }

            Logger.log('Glaze: hide');
            TrackingManager.setWaitingVTC(this.model);

            this.animateOut(settings.animate_out, function(){
                this.undelegateResizeListener();
                this.isShown = false;
                $('html,body').stop();
                $('body').removeClass('glaze-effect');
                this.removeAdditionalCss();
                this.executePostJS();
                this.parent.onCloseGlaze(this.model, converted);
                this.trackAbandon();
                _.isFunction(cb) && cb();
                this.parent.scrollOrResize();
            });
        },

        smoothTransitionGlaze: function(id, options) {
            options = options || {};

            if (!options.ignore_close_action) {
                Logger.log('Glaze: smooth hide');
                _.each(this.elements, function(element){
                    element.remove();
                    element = null;
                });
                this.removeAdditionalCss();

                this.elements = [];

                var settings = this._getSettings();

                if (settings.close_action_js) {
                    Logger.log('Glaze: execute close javascript action');
                    try {
                        eval(settings.close_action_js);
                    } catch(err) {
                        console.warn('Error in close javascript action');
                        console.warn(err);
                    }

                    if (settings.close_action_js_delay && settings.close_action_js_delay > 0) {
                        setTimeout(_.bind(function(){
                            this.smoothTransitionGlaze(id, {ignore_close_action: true})
                        }, this), settings.close_action_js_delay);
                        return;
                    }
                }
            }

            var singleTagAction = settings.universal_action_js;
            if (singleTagAction) {
                singleTagAction = MiscUtils.prepareActionCode(
                    singleTagAction,
